CREATE DATABSE JOBS;

USE JOBS;

CREATE TABLE USUARIO (
    ID_USUARIO INT  PRIMARY KEY AUTO_INCREMENT,
    NOME VARCHAR(100) NOT NULL,
    CPF VARCHAR(15) NOT NULL,
    GENERO VARCHAR(30),
    NASCIMENTO  INT(10) NOT NULL,
    ESTADO_CIVIL VARCHAR(20),
    FONE VARCHAR(15),
    MOBILE VARCHAR(15),
    EMAIL VARCHAR(100),
    CEP VARCHAR(9),
    LOGRADOURO(100),
    NUMERO VARCHAR(6),
    COMPLEMENTO VARCHAR(10),
    BAIRRO VARCHAR(20),
    CIDADE VARCHAR(50),
    ESTADO CHAR(2),
);

